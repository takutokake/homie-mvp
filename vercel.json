{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/web/(.*)",
      "dest": "/web/$1"
    },
    {
      "src": "/signup.html",
      "dest": "/web/signup.html"
    },
    {
      "src": "/complete-profile.html",
      "dest": "/web/complete-profile.html"
    },
    {
      "src": "/login.html",
      "dest": "/web/login.html"
    },
    {
      "src": "/",
      "dest": "/web/index.html"
    }
  ]
}
